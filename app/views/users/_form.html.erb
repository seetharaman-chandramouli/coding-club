<div class="row">
	<div class="col-sm-12">
		<%= form_for(@user)  do |f| %>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :first_name %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :first_name, class: "form-control", placeholder: "Enter Firstname", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :last_name %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :last_name, class: "form-control", placeholder: "Enter Lastname", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :mobile_number %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :mobile_number, class: "form-control", placeholder: "Enter mobilenumber", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :age %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :age, class: "form-control", placeholder: "Enter age", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :batch %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :batch, class: "form-control", placeholder: "Enter batch (eg. 2016-2020)", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :degree %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :degree, class: "form-control", placeholder: "Enter degree", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :collegename %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :college_name, class: "form-control", placeholder: "Enter college name", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :register_number %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :register_number, class: "form-control", placeholder: "Enter register number", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :company_name %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :company_name, class: "form-control", placeholder: "Enter company name", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :designation %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :designation, class: "form-control", placeholder: "Enter designation", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :location %>
				</div>
				<div class="col-sm-8">
					<%= f.text_field :location, class: "form-control", placeholder: "Enter location", autofocus: true %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :email%>
				</div>
				<div class="col-sm-8">
					<%= f.email_field :email,class:"form-control",placeholder: "Enter email" %>
				</div>
			</div>
			<div class="form-group">
				<div class="control-label col-sm-2">
					<%= f.label :password%>
				</div>
				<div class="col-sm-8">
					<%= f.password_field :password,class:"form-control",placeholder: "Enter password" %>
				</div>
			</div>
			<div class="form-group">
				<div class="col-sm-8 col-sm-offset-2">
					<%= f.submit %>
				</div>
			</div>

		<%end%>
	</div>
</div>